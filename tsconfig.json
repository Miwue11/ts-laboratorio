{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "lib": ["ESNext", "DOM"],
    "moduleResolution": "Node",
    "strict": true,
    "resolveJsonModule": true,
    "isolatedModules": false,
    "esModuleInterop": true,
    "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "skipLibCheck": true
  },
  "include": [
    "src",
    "apuntes/minijuego de acertar/lab-2.ts",
    "apuntes/lab-1.ts",
    "apuntes/bingo/model.ts",
    "apuntes/bingo/motor.ts",
    "apuntes/bingo/shell.ts"
  ]
}
